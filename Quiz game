--SCRIPT DOOR

WRONG ANSWER
script.Parent.doorA.Touched:Connect(function(player)
	player.Parent.Humanoid.Health = 0
end)

WRONG ANSWER
script.Parent.doorB.Touched:Connect(function(player)
	player.Parent.Humanoid.Health = 0
end)

TRUE ANSWER
Unchecklist Cancollide

--SCRIPT POINT
points = 0
onTouched = true
-- kalau ini terlalu susah gaperlu ditampilin point nya
game.Workspace.wall1.doorC.Touched:Connect(function(player)
	if player.Parent:FindFirstChild("Humanoid") and onTouched == true then
		points += 1
		script.Parent.ScreenGui.Frame.textscore.Text = points
		game.Workspace.wall1.doorC.BrickColor = BrickColor.new("Bright green")
		onTouched = false
		wait(3)
		onTouched = true
		game.Workspace.wall1.doorC.BrickColor = BrickColor.new("Pastel blue-green")
	end 
end)

